{
	"compilerOptions": {
		"strict": true,
		"lib": ["DOM", "ES2021"],
		"moduleResolution": "Node",
		"jsx": "preserve",
		"jsxFactory": "h",
		"jsxFragmentFactory": "Fragment",
		"jsxImportSource": "preact",
		"types": ["preact", "chrome"],
		"noEmit": true,
		"skipLibCheck": true
	},
	"include": ["./**/*.tsx"]
}
